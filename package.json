{
  "name": "autogit-plugin-rollback",
  "description": "A plugin for rolling back the history to that of the origin and discarding local changes.",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck",
    "compile:watch": "tsc --skipLibCheck --watch",
    "lint": "tslint \"src/**/*.ts*\" --force",
    "prepublish": "npm run clean && npm run lint && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/autogit-plugin-rollback/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/autogit-plugin-rollback.git"
  },
  "keywords": [
    "autogit",
    "plugin",
    "rollback",
    "revert",
    "reset"
  ],
  "dependencies": {
    "listr-input": "^0.1.3",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "simple-git": "^1.105.0"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "rimraf": "^2.6.0",
    "tslint": "^4.4.2",
    "typescript": "^3.0.3"
  }
}
